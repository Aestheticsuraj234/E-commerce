{
    "version": 2,
    "builds": [
      { "src": "next.config.js", "use": "@vercel/next" },
      { "src": "api/**/*.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" }
    ],
    "env": {
      "MONGO_URL": "mongodb://localhost:27017/rollend"
    }
  }